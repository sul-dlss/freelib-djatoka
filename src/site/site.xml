<?xml version="1.0" encoding="UTF-8"?>
<project name="FreeLib-Djatoka" xmlns="http://maven.apache.org/DECORATION/1.3.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd">

  <bannerLeft>
    <name>FreeLib-Djatoka</name>
    <href>http://projects.freelibrary.info/freelib-djatoka</href>
  </bannerLeft>

  <publishDate position="bottom" format="yyyy-MM-dd" />
  <version position="bottom" />
  <googleAnalyticsAccountId>${ga.accountId}</googleAnalyticsAccountId>

  <skin>
    <groupId>lt.velykis.maven.skins</groupId>
    <artifactId>reflow-maven-skin</artifactId>
    <version>1.0.0</version>
  </skin>

  <custom>
    <reflowSkin>
      <protocolRelativeURLs>true</protocolRelativeURLs>
      <smoothScroll>true</smoothScroll>
      <theme>site</theme>
      <brand>
        <name>FreeLibrary Projects</name>
        <href>http://freelibrary.info</href>
      </brand>
      <slogan>A fork of the aDORe-Djatoka JPEG2000 image server</slogan>
      <titleTemplate>%2$s | %1$s</titleTemplate>
      <toc>false</toc> <!-- top -->
      <tocTopMax>6</tocTopMax>
      <topNav>Read the Docs</topNav>
      <bottomNav maxSpan="16">
        <column>Project Links|Build Status</column>
        <column>Contributions|Donations</column>
        <column>Developer Reports</column>
        <column>reports|Maven Theme|Site Copyright</column>
      </bottomNav>
      <pages>
        <index project="freelib-djatoka">
          <titleTemplate>FreeLibrary Djatoka</titleTemplate>
          <breadcrumbs>false</breadcrumbs>
          <toc>false</toc>
          <markPageHeader>true</markPageHeader>
          <sections>
            <columns>2</columns>
            <body />
          </sections>
        </index>
      </pages>
      <skinAttribution>false</skinAttribution>
    </reflowSkin>
  </custom>

  <body>
    <head>
      <link rel="author"
        href="https://plus.google.com/106980687849423472398/about" />
    </head>

    <links>
      <item name="GitHub Page" href="https://github.com/ksclarke/freelib-djatoka"
        target="_new" />
      <item name="Download Zip"
        href="https://github.com/ksclarke/freelib-djatoka/archive/master.zip" />
    </links>

    <!-- top navbar -->
    <menu name="Read the Docs">
      <item name="Introduction" href="index.html#" />
      <item name="Javadocs" href="apidocs/index.html" target="_new" />
      <item name="Issues Queue" target="_new"
        href="https://github.com/ksclarke/freelib-djatoka/issues" />
      <item name="License" href="license.html" />
    </menu>

    <!-- bottom navbar -->
    <menu name="Project Links">
